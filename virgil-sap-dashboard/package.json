{
  "name": "virgil-sap-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:all": "concurrently \"next dev --turbopack\" \"cd ../deck-template && source venv311/bin/activate && python3 template_api.py\"",
    "dev": "concurrently \"next dev --turbopack\" \"cd ../deck-template && source venv311/bin/activate && python3 template_api.py\"",
    "dev:frontend": "next dev --turbopack",
    "dev:backend": "cd ../deck-template && source venv311/bin/activate && python3 template_api.py",
    "build": "next build",
    "start": "next start -p 3000 -H 0.0.0.0",
    "start:python": "cd ../deck-template && .venv/bin/python -m uvicorn template_api:app --host 0.0.0.0 --port 8000",
    "start:all": "concurrently -k -s first \"PYTHON_TEMPLATE_API_URL=http://localhost:8000 next start\" \"npm run start:python\"",
    "start:deck": "cd ../deck-template && source venv311/bin/activate && python3 template_api.py",
    "start:all:deck": "concurrently -k -s first \"PYTHON_TEMPLATE_API_URL=http://localhost:8000 next start\" \"npm run start:deck\"",
    "lint": "next lint",
    "migrate": "tsx src/lib/migrations/run-migrations.ts",
    "migrate:users": "tsx src/lib/migrations/run-user-migrations.ts",
    "setup:admin": "tsx src/lib/setup-admin.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
